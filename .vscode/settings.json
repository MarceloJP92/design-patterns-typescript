{
  "window.zoomLevel": 0,
  "code-runner.executorMap": {
    "typescript": "node --no-warnings --loader ts-node/esm --no-deprecation",
    "python": "python"
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.fixAll": true
  },
  "cSpell.enabled": true,
  "code-runner.runInTerminal": true,
  "code-runner.ignoreSelection": true
}
